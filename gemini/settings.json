{
  "autoAccept": true,
  "allowedTools": [
    "LSTool",
    "ReadFileTool",
    "WriteFileTool",
    "GrepTool",
    "GlobTool",
    "EditTool",
    "ReadManyFilesTool",
    "WebFetchTool",
    "WebSearchTool",
    "MemoryTool",
    "ShellTool"
  ],
  "excludeTools": [
    "ShellTool(git commit)",
    "ShellTool(git push)",
    "ShellTool(git pull)",
    "ShellTool(git fetch)",
    "ShellTool(git merge)",
    "ShellTool(git rebase)",
    "ShellTool(git reset)",
    "ShellTool(git checkout)",
    "ShellTool(git switch)",
    "ShellTool(git cherry-pick)",
    "ShellTool(git revert)",
    "ShellTool(git tag)",
    "ShellTool(git remote)",
    "ShellTool(git clone)",
    "ShellTool(git init)",
    "ShellTool(git rm)",
    "ShellTool(git mv)"
  ],
  "mcpServers": {
    "nx-mcp": {
      "command": "npx",
      "args": ["nx-mcp@latest", "."]
    },
    "context7": {
      "httpUrl": "https://mcp.context7.com/mcp"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    }
  },
  "telemetry": {
    "enabled": false
  },
  "usageStatisticsEnabled": false,
  "chatCompression": {
    "contextPercentageThreshold": 0.8
  },
  "ideMode": true,
  "selectedAuthType": "oauth-personal"
}
